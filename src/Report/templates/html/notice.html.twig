
<div class="row result-group">
  <div class="col-sm-12 col-md-10">
    <h3 id="{{ result.id }}">{{ result.title }}</h3>
    <p>{{ result.description | raw }}</p>
    <div class="panel panel-info">
      <div class="panel-body">
        {{ result.success | raw }}
      </div>
    </div>
  </div>
</div>
